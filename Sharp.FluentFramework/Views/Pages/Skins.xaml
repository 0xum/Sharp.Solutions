<Page x:Class="Imortal.FluentFramework.Views.Pages.Skins"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Imortal.FluentFramework.Views.Pages"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:wpfui="http://schemas.lepo.co/wpfui/2022/xaml"
      Title="Skin"
      d:DesignHeight="450"
      d:DesignWidth="800"
      Style="{StaticResource UiPageScrollable}"
      mc:Ignorable="d">
    <Grid>
        <StackPanel HorizontalAlignment="Stretch"
                    Orientation="Horizontal">
            <StackPanel MinWidth="450"
                        HorizontalAlignment="Stretch">
                <wpfui:CardExpander Margin="0,0,0,5"
                                    Icon="CubeAdd20"
                                    Header="Hello, World!">
                    <GroupBox BorderThickness="0"
                              x:Name="MainGp"></GroupBox>
                </wpfui:CardExpander>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
